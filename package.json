{
  "name": "jincor-backend-notify",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "nodemon -w ./src -e ts --exec ts-node src/app",
    "lint": "tslint 'src/**/*.ts'",
    "lintFix": "tslint --fix 'src/**/*.ts'",
    "build": "tsc -p tsconfig.build.json --outDir dist",
    "serve": "node dist/app.js"
  },
  "nyc": {
    "exclude": [
      "src/**/*.spec.ts"
    ]
  },
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "dotenv": "4.0.0",
    "lru-cache": "4.1.1",
    "mailcomposer": "4.0.2",
    "mailgun-js": "0.14.1",
    "node-mailjet": "3.2.1",
    "nsqjs": "^0.10.0",
    "winston": "2.4.0"
  },
  "devDependencies": {
    "@types/nsqjs": "0.9.0",
    "@types/winston": "2.3.7",
    "nodemon": "^1.14.11",
    "ts-node": "4.1.0",
    "tslint": "5.9.1",
    "tslint-config-standard": "7.0.0",
    "typescript": "2.6.2"
  }
}
